    <!-- start section -->
    <section class="wow fadeIn">
        <div class="container">
            @foreach (var aboutItem in Model.DepsCards.Take(1).ToList())
            {
                <div class="row equalize md-equalize-auto">
                    @foreach (var image in aboutItem.DepCardPhotos)
                    {
                        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12 text-center display-table xs-margin-15px-bottom wow fadeIn">
                            <div class="display-table-cell vertical-align-middle">
                                <img src="/Uploads/@image.PhotoName" alt="" class="width-100">
                            </div>
                        </div>
                    }

                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 no-padding display-table md-margin-five-top sm-text-center xs-no-margin-top wow fadeIn" data-wow-delay="0.4s">
                        <div class="display-table-cell vertical-align-middle padding-twelve-lr md-padding-15px-lr xs-text-center xs-padding-five-lr xs-padding-ten-top width-100">
                            <h4 class="font-weight-600 alt-font text-extra-dark-gray letter-spacing-minus-1">@aboutItem.Title</h4>
                            <p class="text-extra-large alt-font font-weight-400">@aboutItem.Name</p>
                            <p>@aboutItem.Text.Substring(0, 50)</p>
                            <a href="about-us-modern.html" class="btn btn-small btn-dark-gray">About Company</a>
                        </div>
                    </div>


                </div>
            }

        </div>
    </section>
    <!-- end section -->
    <section class="wow fadeIn">
        <div class="container">
            <div class="row">
                @foreach (var item in Model.HonorNumbers)
                {
                    <!-- start counter item -->
                    <div class="col-md-3 col-sm-6 col-xs-12 sm-margin-five-bottom sm-padding-ten-left xs-padding-15px-left xs-margin-30px-bottom text-center wow fadeInDown">
                        <h5 class="text-extra-dark-gray font-weight-600 no-margin timer  alt-font" data-speed="2000" data-to="@item.Number">@item.Number</h5>
                        <span class="display-block margin-three-bottom">@item.Number</span>
                        <div class="separator-line-verticle-large bg-deep-pink display-inline-block"></div>
                    </div>
                    <!-- end counter item -->
                }

            </div>
        </div>
    </section>
    <!-- end counter section -->
    <!-- start slider section -->
    <section class="wow fadeIn no-padding">
        <div class="swiper-auto-height-container position-relative width-100">
            <div class="swiper-wrapper overflow-hidden">
                <!-- start slider item -->
                @foreach (var item in Model.MainPageSliders)
                {
                    <div class="swiper-slide padding-100px-all cover-background position-relative xs-padding-20px-all" style="background-image:url(/uploads/@item.Photo)">
                        <div class="position-relative width-40 md-width-60 sm-width-85 xs-width-100 display-inline-block slide-banner last-paragraph-no-margin">
                            <div class="padding-80px-all bg-black-opacity sm-padding-40px-all xs-padding-30px-all xs-text-center xs-width-100">
                                <h5 class="alt-font text-white width-90 sm-width-100">@item.Title</h5>
                                <p class="width-90 sm-width-100">@item.Text</p>
                                <a href="services-classic.html" class="margin-35px-top sm-margin-15px-top btn btn-small btn-white">Explore services</a>
                            </div>
                        </div>
                    </div>
                }


            </div>

            <div class="navigation-area" style="left: 524px; bottom: 100px;">
                <div class="swiper-button-next swiper-next-style4 bg-primary text-white"><i class="fas fa-arrow-up" aria-hidden="true"></i></div>
                <div class="swiper-button-prev swiper-prev-style4"><i class="fas fa-arrow-down" aria-hidden="true"></i></div>
            </div>

        </div>
    </section>
    <!-- end slider section -->
    <!-- start masonry portfolio section -->
    <section class="no-padding-bottom wow fadeIn">
        <div class="container-fluid no-padding">
            <div class="row text-center">
                <div class="col-md-12">
                    <p class="alt-font margin-5px-bottom text-uppercase text-small">Best of Ours</p>
                    <h5 class="text-uppercase alt-font text-extra-dark-gray margin-20px-bottom font-weight-700 sm-width-100 xs-width-100">New Projects</h5>
                    <span class="separator-line-horrizontal-medium-light2 bg-deep-pink display-table margin-auto width-100px"></span>
                </div>
            </div>
            <div class="row no-margin">
                <div class="filter-content overflow-hidden margin-100px-top sm-margin-75px-top xs-margin-50px-top">
                    <ul class="portfolio-grid work-4col hover-option2 gutter-small">
                        <li class="grid-sizer"></li>
                        @foreach (var project in Model.NewProjects)
                        {
                            <!-- start portfolio item -->
                            <li class="grid-item wow fadeInUp">
                                <a href="single-project-page-01.html">
                                    <figure>
                                        <div class="portfolio-img bg-deep-pink"><img src="/uploads/@project.Photo" alt="" /></div>
                                        <figcaption>
                                            <div class="portfolio-hover-main text-left">
                                                <div class="portfolio-hover-box vertical-align-bottom">
                                                    <div class="portfolio-hover-content position-relative last-paragraph-no-margin">
                                                        <div class="bg-black margin-six-bottom separator-line-horrizontal-medium-light2"></div>
                                                        <span class="font-weight-600 line-height-normal alt-font text-white text-uppercase margin-one-half-bottom display-block">@project.Title</span>

                                                        <p class="text-white text-uppercase text-extra-small">@project.Departments.DepName</p>

                                                    </div>
                                                </div>
                                            </div>
                                        </figcaption>
                                    </figure>
                                </a>
                            </li>
                            <!-- end portfolio item -->
                        }


                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- end masonry portfolio section -->
